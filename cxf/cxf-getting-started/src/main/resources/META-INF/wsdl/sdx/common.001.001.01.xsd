	<xs:complexType name="AddressFull">
		<xs:sequence>
			<xs:element minOccurs="0" name="Code" type="AddressCode">
				<xs:annotation>
					<xs:documentation>Kód adresy - zatím povinné pro import adres společností ze Siebel (jednoznačný identifikátor). V případě adresy uživatele, pokud je při importu nevyplněný, jedná se o novou adresu, jinak o změnu stávající.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Name" type="AddressName">
				<xs:annotation>
					<xs:documentation>Název adresy (zatím povinné pouze pro uživatele)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="AddressType">
				<xs:annotation>
					<xs:documentation>Typ adresy, uživatelé mají pouze doručovací adresy</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="AddressStatus">
				<xs:annotation>
					<xs:documentation>Stav adresy: platná, neplatná.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Primary" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Pro doručovací adresu příznak, že je primární</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Line1" type="AddressLine">
				<xs:annotation>
					<xs:documentation>1. řádek adresy uživatele. Povinné pro uživatele. U společnosti nemá být.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Line2" type="AddressLine">
				<xs:annotation>
					<xs:documentation>2. řádek adresy uživatele. Nepovinné pro uživatele. U společnosti nemá být.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Street" type="Street">
				<xs:annotation>
					<xs:documentation>Ulice</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="StreetNumber" type="StreetNumber">
				<xs:annotation>
					<xs:documentation>Číslo domu popisné</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="StreetNumber2" type="StreetNumber">
				<xs:annotation>
					<xs:documentation>Číslo domu orientační</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="City" type="City">
				<xs:annotation>
					<xs:documentation>Město</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="PostalCode" type="PostalCode">
				<xs:annotation>
					<xs:documentation>Psč</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Country" type="CountryCode">
				<xs:annotation>
					<xs:documentation>Země</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
